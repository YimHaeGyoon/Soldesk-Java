package chapter06;

public class TeamMember_05 {

	private TeamMember[] member = new TeamMember[5];

	public void init() {

		member[0] = new TeamMember("신인철", "남학생");
		member[1] = new TeamMember("김관호", "남학생");
		member[2] = new TeamMember("문원주", "남학생");
		member[3] = new TeamMember("임해균", "남학생");
		member[4] = new TeamMember("유연우", "여학생");
	}

}
