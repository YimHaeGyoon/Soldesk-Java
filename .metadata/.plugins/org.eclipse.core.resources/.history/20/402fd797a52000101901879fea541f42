	static int a = 1;

	// 생성자

	// 메서드

	// 내부 클래스
	public class In {

		// 멤버변수

		// 생성자

		// 메서드

	}// In class

}// out class

public class NonStatic_01 {

	public static void main(String[] args) {

		// #1 Out class 객체
